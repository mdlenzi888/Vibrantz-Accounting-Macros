        "=IF(RC[-3]=""-"",""-"",IF(+'JE Template'!R[13]C[7]="""",""-"",+'JE Template'!R[13]C[7]))"
    Range("F1").Select
    ActiveCell.FormulaR1C1 = _
        "=IF(RC[-5]=""-"",""-"",IF(+'JE Template'!R[13]C[9]="""",""-"",+'JE Template'!R[13]C[9]))"
    Range("G1").Select
    ActiveCell.FormulaR1C1 = _
        "=IF(A1=""-"",""-"",IF(+'JE Template'!M14="""",""-"",+'JE Template'!M14))"
    Range("G1").Select
    ActiveCell.FormulaR1C1 = _
